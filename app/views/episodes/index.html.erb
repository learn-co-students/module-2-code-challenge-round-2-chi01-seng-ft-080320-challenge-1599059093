<h1>All Episodes</h1>

<ul>
  <% @episodes.each do |episode| %>
    <li><%= link_to "Episode #{episode.number} - #{episode.date.strftime("%B %d, %Y")}", episode_path(episode)%></li>
  <% end %>
</ul>
<br>
<button><%= link_to "Log a New Guest Appearance", new_appearance_path, {style: "text-decoration: none" } %></button>